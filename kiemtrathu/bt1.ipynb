    "import math\n",
    "def euclidean_distance(x, y):\n",
    "    distance = 0\n",
    "    for i in range(len(x)):\n",
    "        distance += (x[i] - y[i])**2\n",
    "    distance = math.sqrt(distance)\n",
    "    #Code here\n",
    "    return distance\n",
    "x = [1,2,3]\n",
    "y = [4,5,6]\n",
    "print(euclidean_distance(x, y)) "
   ]
